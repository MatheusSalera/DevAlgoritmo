#include <stdio.h>
#include <stdlib.h>



int main() {
    int array[5];  

    for (int i = 0; i < 5; i++) {
        printf("Digite um 5 valores: ", i + 1);
        scanf("%d", &array[i]);
    }


    printf("\nValores digitados:\n");
    for (int i = 0; i < 5; i++) {
        printf("%d\n ", array[i]);
    }

   
	system("pause");	
	return 0;
}
