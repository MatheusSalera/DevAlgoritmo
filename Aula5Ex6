#include <stdio.h>
#include <stdlib.h>



int main() {
    int matriz[3][3];


    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            printf("Digite o elemento da matriz: ", i, j);
            scanf("%d", &matriz[i][j]);
        }
    }

   
    printf("\nMatriz 3x3:\n");
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            printf("%d\t", matriz[i][j]);  
        }
        printf("\n");  
    }


	system("pause");
	return 0;
}
