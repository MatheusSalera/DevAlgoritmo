#include <stdio.h>
#include <stdlib.h>

int main() {
    int numeros[10];
    int soma = 0;
    int positivos = 0;

    for (int i = 0; i < 10; i++) {
        printf("Digite o numero: ", i + 1);
        scanf("%d", &numeros[i]);
        soma += numeros[i];
        if (numeros[i] > 0) {
            positivos++;
        }
    }

    printf("\n Soma de todos os elementos: %d\n", soma);
    printf("Quantidade de numeros positivos: %d\n", positivos);


	system("pause");
	return 0;
}
